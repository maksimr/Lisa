{
  "name": "playground",
  "version": "0.0.0",
  "main": "./src/index.js",
  "scripts": {
    "start": "webpack serve --mode development -d eval-source-map --disable-host-check --history-api-fallback --host 0.0.0.0 --public ${SANDBOX_URL:-''}",
    "build": "webpack -p",
    "test": "jest --passWithNoTests",
    "test:type": "tsc",
    "lint": "eslint src/"
  },
  "dependencies": {
    "@maksimr/ui": "0.0.27",
    "immer": "8.0.4",
    "normalizr": "3.6.1",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "typescript": "4.2.3",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.2",
    "@babel/core": "7.13.10",
    "@babel/preset-env": "7.13.10",
    "@babel/eslint-parser": "7.13.10",
    "@babel/preset-react": "7.12.13",
    "eslint-plugin-react": "7.22.0",
    "babel-loader": "8.2.2",
    "css-loader": "5.1.3",
    "html-webpack-plugin": "5.3.1",
    "webpack": "5.27.0",
    "webpack-cli": "4.5.0",
    "webpack-dev-server": "3.11.2",
    "eslint": "7.22.0",
    "jest": "26.6.3",
    "jest-cli": "26.6.3"
  },
  "eslintConfig": {
    "parser": "@babel/eslint-parser",
    "plugins": [
      "react"
    ],
    "parserOptions": {
      "requireConfigFile": false,
      "ecmaVersion": 8,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "no-undef": "error",
      "no-unused-vars": "error",
      "react/jsx-uses-react": 1,
      "react/jsx-uses-vars": 1
    }
  }
}
